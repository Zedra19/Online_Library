<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <View
        android:id="@+id/rectangle_1"
        android:layout_width="393dp"
        android:layout_height="89dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:background="#163062"
        app:layout_constraintTop_toTopOf="parent" />

    <View
        android:id="@+id/carticon"
        android:layout_width="24dp"
        android:layout_height="21.33333dp"
        android:layout_centerHorizontal="true"
        android:layout_centerVertical="true"
        android:layout_marginStart="25dp"
        android:layout_marginTop="45dp"
        android:background="@drawable/icon_cart"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/taskbar"
        android:layout_width="393dp"
        android:layout_height="77dp"
        android:background="@drawable/taskbar"
        android:gravity="center_horizontal|center"
        android:textAppearance="@style/poppins"
        android:textSize="12sp"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="776dp" />

    <TextView
        android:id="@+id/reviewheader"
        android:layout_width="82dp"
        android:layout_height="21dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginTop="44dp"
        android:gravity="center_horizontal|bottom"
        android:text="@string/review"
        android:textAppearance="@style/beranda"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/noreview"
        android:layout_width="312dp"
        android:layout_height="27dp"
        android:layout_centerHorizontal="true"
        android:layout_centerVertical="true"
        android:gravity="center_horizontal|top"
        android:text="belum ada review"
        android:textAppearance="@style/nobook"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <View
        android:id="@+id/chaticon"
        android:layout_width="24dp"
        android:layout_height="21.33333dp"
        android:layout_centerHorizontal="true"
        android:layout_centerVertical="true"
        android:layout_marginStart="26dp"
        android:layout_marginBottom="97dp"
        android:background="@drawable/icon_chat"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageView
        android:id="@+id/imageHome"
        android:layout_width="24dp"
        android:layout_height="22dp"
        app:srcCompat="@drawable/home_not"
        tools:layout_editor_absoluteX="33dp"
        tools:layout_editor_absoluteY="798dp" />

    <ImageView
        android:id="@+id/imageKatalog"
        android:layout_width="11dp"
        android:layout_height="12dp"
        app:layout_constraintEnd_toStartOf="@+id/taskbar"
        app:layout_constraintHorizontal_bias="0.685"
        app:layout_constraintStart_toEndOf="@+id/taskbar"
        app:srcCompat="@drawable/icon_book"
        tools:layout_editor_absoluteY="808dp" />

    <ImageView
        android:id="@+id/imageDipinjam"
        android:layout_width="24dp"
        android:layout_height="22dp"
        app:srcCompat="@drawable/icon_bookmark"
        tools:layout_editor_absoluteX="184dp"
        tools:layout_editor_absoluteY="799dp" />

    <ImageView
        android:id="@+id/imageReview"
        android:layout_width="24dp"
        android:layout_height="22dp"
        app:srcCompat="@drawable/review_selected"
        tools:layout_editor_absoluteX="262dp"
        tools:layout_editor_absoluteY="799dp" />

    <ImageView
        android:id="@+id/imageAccount"
        android:layout_width="24dp"
        android:layout_height="22dp"
        app:srcCompat="@drawable/icon_user"
        tools:layout_editor_absoluteX="342dp"
        tools:layout_editor_absoluteY="798dp" />

    <TextView
        android:id="@+id/hometext"
        android:layout_width="36dp"
        android:layout_height="15dp"
        android:gravity="top"
        android:text="@string/home"
        android:textAppearance="@style/taskbar_ganyala"
        tools:layout_editor_absoluteX="28dp"
        tools:layout_editor_absoluteY="823dp" />

    <TextView
        android:id="@+id/katalogtext"
        android:layout_width="48dp"
        android:layout_height="15dp"
        android:gravity="top"
        android:text="@string/katalog"
        android:textAppearance="@style/taskbar_ganyala"
        tools:layout_editor_absoluteX="100dp"
        tools:layout_editor_absoluteY="823dp" />

    <TextView
        android:id="@+id/dipinjamtext"
        android:layout_width="48dp"
        android:layout_height="15dp"
        android:gravity="top"
        android:text="@string/dipinjam"
        android:textAppearance="@style/taskbar_ganyala"
        tools:layout_editor_absoluteX="172dp"
        tools:layout_editor_absoluteY="823dp" />

    <TextView
        android:id="@+id/reviewtext"
        android:layout_width="48dp"
        android:layout_height="15dp"
        android:gravity="top"
        android:text="@string/review2"
        android:textAppearance="@style/taskbar_nyala"
        tools:layout_editor_absoluteX="255dp"
        tools:layout_editor_absoluteY="823dp" />

    <TextView
        android:id="@+id/accounttext"
        android:layout_width="48dp"
        android:layout_height="15dp"
        android:gravity="top"
        android:text="@string/akun"
        android:textAppearance="@style/taskbar_ganyala"
        tools:layout_editor_absoluteX="331dp"
        tools:layout_editor_absoluteY="823dp" />

</androidx.constraintlayout.widget.ConstraintLayout>