<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <View
        android:id="@+id/rectangle_1"
        android:layout_width="393dp"
        android:layout_height="89dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:background="#163062"
        app:layout_constraintTop_toTopOf="parent" />


    <View
        android:id="@+id/profile"
        android:layout_width="25dp"
        android:layout_height="25dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="20dp"
        android:layout_marginTop="192dp"
        android:background="@drawable/chatprofile"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <View
        android:id="@+id/profile2"
        android:layout_width="25dp"
        android:layout_height="25dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="20dp"
        android:layout_marginTop="340dp"
        android:background="@drawable/chatprofile"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <View
        android:id="@+id/profile3"
        android:layout_width="25dp"
        android:layout_height="25dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="20dp"
        android:layout_marginTop="415dp"
        android:background="@drawable/chatprofile"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <View
        android:id="@+id/chat1"
        android:layout_width="255dp"
        android:layout_height="46dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginTop="130dp"
        android:layout_marginEnd="20dp"
        android:background="@drawable/chat_bubble_blue"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


    <View
        android:id="@+id/chat"
        android:layout_width="255dp"
        android:layout_height="46dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginTop="28dp"
        android:layout_marginEnd="20dp"
        android:background="@drawable/chat_bubble_blue"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/chat2" />

    <View
        android:id="@+id/chat5"
        android:layout_width="257dp"
        android:layout_height="62dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginTop="28dp"
        android:layout_marginEnd="25dp"
        android:background="@drawable/chat_bubble_blue"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/chat4" />

    <TextView
        android:id="@+id/chatt1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:text="Hallo!"
        android:textAppearance="@style/chat1"
        app:layout_constraintBottom_toBottomOf="@+id/chat1"
        app:layout_constraintStart_toStartOf="@+id/chat1"
        app:layout_constraintTop_toTopOf="@+id/chat1" />

    <TextView
        android:id="@+id/chatt8"
        android:layout_width="222dp"
        android:layout_height="44dp"
        android:layout_marginStart="8dp"
        android:text="Iya, saya sangat tertarik dengan buku itu"
        android:textAppearance="@style/chat1"
        app:layout_constraintBottom_toBottomOf="@+id/chat5"
        app:layout_constraintStart_toStartOf="@+id/chat5"
        app:layout_constraintTop_toTopOf="@+id/chat5"
        app:layout_constraintVertical_bias="0.444" />

    <TextView
        android:id="@+id/chatt3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginTop="14dp"
        android:layout_marginBottom="17dp"
        android:text="Apakah buku Rindu dari Tere Liye ada?"
        android:textAppearance="@style/chat1"
        app:layout_constraintBottom_toBottomOf="@+id/chat"
        app:layout_constraintStart_toStartOf="@+id/chat"
        app:layout_constraintTop_toTopOf="@+id/chat"
        app:layout_constraintVertical_bias="0.0" />

    <View
        android:id="@+id/chat2"
        android:layout_width="255dp"
        android:layout_height="46dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="60dp"
        android:layout_marginTop="28dp"
        android:background="@drawable/chat_bubble_white"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/chat1" />

    <View
        android:id="@+id/chat3"
        android:layout_width="255dp"
        android:layout_height="46dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="60dp"
        android:layout_marginTop="28dp"
        android:background="@drawable/chat_bubble_white"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/chat" />

    <View
        android:id="@+id/chat4"
        android:layout_width="255dp"
        android:layout_height="46dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="60dp"
        android:layout_marginTop="28dp"
        android:background="@drawable/chat_bubble_white"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/chat3" />

    <TextView
        android:id="@+id/chatt2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:text="Hai!"
        android:textAppearance="@style/buku_trendi"
        app:layout_constraintBottom_toBottomOf="@+id/chat2"
        app:layout_constraintStart_toStartOf="@+id/chat2"
        app:layout_constraintTop_toTopOf="@+id/chat2"
        app:layout_constraintVertical_bias="0.423" />

    <TextView
        android:id="@+id/chatt4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:text="ada"
        android:textAppearance="@style/buku_trendi"
        app:layout_constraintBottom_toBottomOf="@+id/chat3"
        app:layout_constraintStart_toStartOf="@+id/chat3"
        app:layout_constraintTop_toTopOf="@+id/chat3" />

    <TextView
        android:id="@+id/chatt6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:text="Apakah anda ingin meminjamnya?"
        android:textAppearance="@style/buku_trendi"
        app:layout_constraintBottom_toBottomOf="@+id/chat4"
        app:layout_constraintStart_toStartOf="@+id/chat4"
        app:layout_constraintTop_toTopOf="@+id/chat4" />

    <TextView
        android:id="@+id/livechat"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginTop="28dp"
        android:gravity="left|top"
        android:text="Livechat"
        android:textAppearance="@style/lexend"
        android:textColor="#E7EBFE"
        android:textSize="22sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <View
        android:id="@+id/plusicon"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_centerHorizontal="true"
        android:layout_centerVertical="true"
        android:layout_marginStart="20dp"
        android:layout_marginBottom="30dp"
        android:background="@drawable/plusicon"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />


    <EditText
        android:id="@+id/editTextText"
        android:layout_width="239dp"
        android:layout_height="43dp"
        android:ems="10"
        android:inputType="text"
        android:text="Type Message"
        tools:layout_editor_absoluteX="72dp"
        tools:layout_editor_absoluteY="780dp" />

    <View
        android:id="@+id/sendicon"
        android:layout_width="32dp"
        android:layout_height="32dp"
        android:layout_centerHorizontal="true"
        android:layout_centerVertical="true"
        android:layout_marginEnd="23.45dp"
        android:layout_marginBottom="25.25dp"
        android:background="@drawable/sendicon"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>